mixin storyImages(story, classes = " col-md-8 offset-md-2")
  .row.position-relative
    .align-items-center(class=classes)
      img.object-fit-cover.img-fluid.rounded-circle.w-10(alt=story.subtitle src=`/images/${story && story.avatarSmall ? story.avatarSmall : story.avatarMedium ? story.avatarMedium : story.avatar }`)

